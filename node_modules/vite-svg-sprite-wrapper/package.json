{
  "name": "vite-svg-sprite-wrapper",
  "type": "module",
  "version": "1.3.2",
  "packageManager": "pnpm@8.15.5",
  "description": "Creating one sprite file on the fly",
  "author": "vshepel <web.shepel@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/vshepel/vite-svg-sprite-wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/vshepel/vite-svg-sprite-wrapper"
  },
  "bugs": "https://github.com/vshepel/vite-svg-sprite-wrapper/issues",
  "keywords": [
    "vite",
    "vite-plugin",
    "svg",
    "svg-sprite"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "dev": "pnpm run build --watch",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "picocolors": "^1.0.0",
    "picomatch": "^4.0.2",
    "svg-sprite": "^2.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.11.6",
    "@types/node": "^20.12.2",
    "@types/picomatch": "^2.3.3",
    "@types/svg-sprite": "^0.0.39",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.7"
  }
}
